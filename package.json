{
  "name": "vendure-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "node --max-old-space-size=8192 -r ts-node/register ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "populate": "ts-node ./src/populate-test-data.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:worker\"",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently --kill-others-on-fail \"npm run start:server\" \"npm run start:worker\"",
    "codegen": "graphql-codegen --config codegen.ts",
    "init-db": "node scripts/init-database.js",
    "setup": "npm run init-db",
    "generate:all-products": "node ./src/strees-test/generate-all-products.js",
    "upload:csv": "node ./src/strees-test/upload-to-storage.js",
    "download:csv": "node ./src/strees-test/download-from-storage.js",
    "import:products": "node --max-old-space-size=16384 --expose-gc -r ts-node/register ./src/strees-test/import-products.ts",
    "import:products:1m": "TARGET_PRODUCTS=1000000 BATCH_SIZE=500 CONCURRENCY=5 node --max-old-space-size=16384 --expose-gc -r ts-node/register ./src/strees-test/import-products-1m.ts",
    "import:products:optimized": "TARGET_PRODUCTS=1000000 BATCH_SIZE=1000 CONCURRENCY=8 node --max-old-space-size=16384 --expose-gc -r ts-node/register ./src/strees-test/import-products.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "@google-cloud/storage": "^7.17.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/graphql": "^13.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@vendure/admin-ui-plugin": "3.4.0",
    "@vendure/asset-server-plugin": "3.4.0",
    "@vendure/common": "3.4.0",
    "@vendure/core": "3.4.0",
    "@vendure/elasticsearch-plugin": "^3.4.0",
    "@vendure/email-plugin": "3.4.0",
    "@vendure/graphiql-plugin": "3.4.0",
    "@vendure/payments-plugin": "^3.4.0",
    "dotenv": "17.2.1",
    "graphql-tag": "^2.12.6",
    "pg": "8.16.3",
    "stripe": "^13.11.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/typescript": "4.1.6",
    "@vendure/cli": "3.4.0",
    "@vendure/create": "^3.4.2",
    "concurrently": "9.2.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
